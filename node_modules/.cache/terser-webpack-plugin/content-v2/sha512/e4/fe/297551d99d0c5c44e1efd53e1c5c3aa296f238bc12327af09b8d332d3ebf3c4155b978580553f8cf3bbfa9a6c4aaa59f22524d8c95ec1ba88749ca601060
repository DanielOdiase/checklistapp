-router\"\nimport App from \"../App\"\nexport default function PrivateRoute({component:Component}) {\n  const { currentUser } = useAuth()\n  \n  return (\n    \n        currentUser ? <App/>: <Navigate to=\"/signin\" />\n      \n        \n  )\n}\n\n  \n  ","   \nimport React from \"react\"\n\nimport { useAuth } from \"../firebase_config\"\nimport { Navigate } from \"react-router\"\n\n\nexport default function Private({children}) {\n  const { currentUser } = useAuth()\n  \n  return (\n    \n        !currentUser? children : <Navigate to=\"/\" />\n      \n        \n  )\n}\n\n  \n  ","import { AuthProvider} from '../firebase_config'\nimport SignIn from './Signin/Signin'\nimport {BrowserRouter as Router,Routes,Route} from 'react-router-dom'\nimport React from 'react'\nimport App from '../App'\nimport SignUp from './SignUp/SignUp'\nimport PrivateRoute from './PrivateRoute'\nimport Private from './Private'\n\nfunction Routing(){\n    return (\n        \n        <Router>\n        <AuthProvider>\n        <Routes>\n        <Route\n          path=\"/\" element={ <PrivateRoute> <App /></PrivateRoute>}/>\n              <Route\n          path=\"/signin\" element={ <Private> <SignIn/></Private>}/>\n              \n              <Route path=\"/signup\" element={<SignUp/>} />\n              \n            </Routes>\n            </AuthProvider>\n            </Router> \n            \n    )\n}\nexport default Routing","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport './index.css';\n\nimport Routing from './components/Routing';\n\n\nReactDOM.render(\n  <React.StrictMode>\n    <Routing />\n  </React.StrictMode>,\n  document.getElementById('root')\n);\n\n"]}}